#include<stdio.h>
#include<string.h>

int main(){

  char s[20],temp;
  i,j;

  printf("Enter the word: ");
  gets(s);

  for(i=0;s[i]!='\0';i++)
  {
      for(j=i+1;s[j]!='\0';j++)
      {
          temp = s[i] ;
          s[i] = s[j] ;
          s[j] = temp ;
          printf("\n %s",s);
      }
  }




}
